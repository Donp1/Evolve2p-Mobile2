{
  "expo": {
    "name": "Evolve2p",
    "slug": "Evolve2p",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "evolve2p",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Logo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      },
      "package": "com.Evolve2p",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/Logo.png",
          "color": "#ffffff",
          "presentationOptions": {
            "badge": true,
            "sound": true,
            "alert": true
          },
          "enableBackgroundRemoteNotification": true
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fe648edc-2920-4da5-8403-0c5a290a348b"
      }
    },
    "owner": "jptech78789898",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fe648edc-2920-4da5-8403-0c5a290a348b",
      "fallbackToCacheTimeout": 0,
      "enabled": true
    }
  }
}
